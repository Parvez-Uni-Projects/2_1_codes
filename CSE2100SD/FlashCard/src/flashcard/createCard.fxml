<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" fx:id="scenePane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="flashcard.CreateCardController">
   <children>
      <Button fx:id="playButton" layoutX="274.0" layoutY="32.0" mnemonicParsing="false" onAction="#playAction" text="Play" />
      <Button fx:id="homeButton" layoutX="43.0" layoutY="32.0" mnemonicParsing="false" onAction="#homeaction" text="Home" />
      <Button fx:id="exitButton" layoutX="499.0" layoutY="32.0" mnemonicParsing="false" onAction="#exitAction" text="Exit" />
      <Label fx:id="question" layoutX="64.0" layoutY="147.0" text="Question" />
      <Label fx:id="answer" layoutX="64.0" layoutY="273.0" text="Answer" />
      <TextField fx:id="questionField" layoutX="133.0" layoutY="143.0" onAction="#questionAction" prefHeight="80.0" prefWidth="343.0" promptText="Add question here" />
      <TextField fx:id="answerField" layoutX="133.0" layoutY="250.0" onAction="#answerAction" prefHeight="80.0" prefWidth="343.0" promptText="Add answer here" />
      <Button fx:id="nextButton" layoutX="133.0" layoutY="345.0" mnemonicParsing="false" onAction="#nextAction" text="Next" />
      <Button fx:id="saveButton" layoutX="377.0" layoutY="345.0" mnemonicParsing="false" onAction="#saveAction" text="Save" />
   </children>
</AnchorPane>
